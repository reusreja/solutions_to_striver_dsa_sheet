#include<bits/stdc++.h>
using namespace std;

int main()
{
   int n,z;
   cin>>n;
   int x=1;
   int y=1;
   if (n == 1) {
     cout << '1';
   } 
   else if (n == 2) {
     cout << '1';
   }
   else {
     for (int i = 3; i <= n; i++) {
       z = x + y;
       x = y;
       y = z;
     }
     cout<<z;
   }

}
